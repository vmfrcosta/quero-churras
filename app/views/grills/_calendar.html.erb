<div class="mt-3 d-flex flex-column align-items-center">
    <div class="container">
      	<div class="form-wrapper" style="width: 300px; margin: 0 auto;">
	        <%= simple_form_for :search, method: "get" do |f| %>
	          <%= f.input :chech_in, as: :string, required: false, input_html: {class: "datepicker"} %>
	          <%= f.input :check_out, as: :string, required: false, input_html: {class: "datepicker"} %>
	          <%= f.input :address , as: :string %>
	          <%= f.button :submit, "Reservar", class: "btn btn-primary" %>
	        <% end %>
    	</div>
	</div>
</div>